{
  "name": "@bizjs/chainkit",
  "version": "0.0.6",
  "description": "chainkit",
  "exports": {
    "./aptos": {
      "require": "./dist/aptos.js",
      "import": "./dist/esm/aptos.js"
    },
    "./bitcoin": {
      "require": "./dist/bitcoin.js",
      "import": "./dist/esm/bitcoin.js"
    },
    "./cardano": {
      "require": "./dist/cardano.js",
      "import": "./dist/esm/cardano.js"
    },
    "./evm": {
      "require": "./dist/evm.js",
      "import": "./dist/esm/evm.js"
    },
    "./starknet": {
      "require": "./dist/starknet.js",
      "import": "./dist/esm/starknet.js"
    },
    "./svm": {
      "require": "./dist/svm.js",
      "import": "./dist/esm/svm.js"
    },
    "./tvm": {
      "require": "./dist/tvm.js",
      "import": "./dist/esm/tvm.js"
    }
  },
  "typesVersions": {
    "*": {
      "aptos": [
        "./dist/aptos.d.ts"
      ],
      "bitcoin": [
        "./dist/bitcoin.d.ts"
      ],
      "cardano": [
        "./dist/cardano.d.ts"
      ],
      "evm": [
        "./dist/evm.d.ts"
      ],
      "starknet": [
        "./dist/starknet.d.ts"
      ],
      "svm": [
        "./dist/svm.d.ts"
      ],
      "tvm": [
        "./dist/tvm.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bizjs/chainkit.git"
  },
  "bugs": {
    "url": "https://github.com/bizjs/chainkit/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "blockchain",
    "chainkit"
  ],
  "author": "hstarorg",
  "license": "MIT",
  "packageManager": "pnpm@10.7.0",
  "dependencies": {
    "@bizjs/chainkit-evm": "workspace:*",
    "@bizjs/chainkit-tvm": "workspace:*",
    "@bizjs/chainkit-svm": "workspace:*",
    "@bizjs/chainkit-aptos": "workspace:*",
    "@bizjs/chainkit-bitcoin": "workspace:*",
    "@bizjs/chainkit-cardano": "workspace:*",
    "@bizjs/chainkit-starknet": "workspace:*"
  }
}
